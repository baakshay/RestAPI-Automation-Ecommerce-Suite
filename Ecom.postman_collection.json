          "listen": "test",
          "script": {
            "exec": [
              "const jsonData = pm.response.json();\r",
              "pm.collectionVariables.set(\"orders\", jsonData.orders[0]);\r",
              "pm.test(\"Check if the order was placed successfully\", ()=>{\r",
              "    pm.expect(jsonData.message).to.eql(\"Order Placed Successfully\");\r",
              "});\r",
              "pm.test(\"Check status code is 201 created\", ()=>{\r",
              "    pm.response.to.have.status(201);\r",
              "});\r",
              "pm.test(\"Check order id\", ()=>{\r",
              "    pm.expect(jsonData.orders[0]).to.eql(pm.collectionVariables.get(\"orders\"));\r",
              "});\r",
              "pm.test(\"Check Prduct order id\", ()=>{\r",
